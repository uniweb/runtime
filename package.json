{
  "name": "@uniweb/runtime",
  "version": "0.6.0",
  "description": "Minimal runtime for loading Uniweb foundations",
  "type": "module",
  "exports": {
    ".": "./src/index.jsx",
    "./ssr": "./dist/ssr.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build:ssr": "vite build --config vite.config.ssr.js",
    "prepublishOnly": "npm run build:ssr"
  },
  "keywords": [
    "uniweb",
    "runtime",
    "react",
    "foundation"
  ],
  "author": "Proximify",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uniweb/runtime.git"
  },
  "homepage": "https://github.com/uniweb/runtime#readme",
  "bugs": {
    "url": "https://github.com/uniweb/runtime/issues"
  },
  "engines": {
    "node": ">=20.19"
  },
  "dependencies": {
    "@uniweb/core": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.5.2",
    "vite": "^7.3.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^6.0.0 || ^7.0.0"
  }
